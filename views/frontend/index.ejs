.rainbow {
  position: relative;
  z-index: 0;
  
 
  border-radius: 10px;
  overflow: hidden;
  padding: 2rem;
}
.rainbow::before {
  content: '';
  position: absolute;
  z-index: -2;
  left: -50%;
  top: -50%;
  width: 200%;
  height: 200%;
  background-color: #f9faf9;
  background-repeat: no-repeat;
  background-size: 20% 50%, 80% 50%;
  background-position: 0 0, 100% 0, 100% 100%, 0 100%;
  background-image: -webkit-gradient(linear, left top, left bottom, from(#ffffff), to(#ffffff)), -webkit-gradient(linear, left top, left bottom, from(#f8f7f6), to(#fcfbf8)), -webkit-gradient(linear, left top, left bottom, from(#faf9f9), to(#faf7f6)), -webkit-gradient(linear, left top, left bottom, from(#fdfeff), to(#fafbfd));
  background-image: linear-gradient(#f9fcfa, #f5faf7), linear-gradient(#696868, #f7f6f3), linear-gradient(#f5f3f3, #faf8f8), linear-gradient(#f4f6fa, #fcfcfd);
  -webkit-animation: rotate 8s linear infinite;
          animation: rotate 8s linear infinite;
}
.rainbow::after {
  content: '';
  position: absolute;
  z-index: -1;
  left: 6px;
  top: 6px;
  width: calc(100% - 12px);
  height: calc(100% - 12px);
  background: white;
  border-radius: 3px;
}

